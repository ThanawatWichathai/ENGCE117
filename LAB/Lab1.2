#include <stdio.h>

void GetSet( int [], int * ) ; //Prototype ประกาศให้รู้ว่ามี function อะไรบ้าง

int main() {                   // โลกของเรา
    
    int data[500], num = 0 ;
    //data = new int[999] ;
    GetSet( data, &num ) ;

    return 0 ;
}//end function 

void GetSet( int data[], int *num ) {  //ต่างโลก                         
    scanf( "%d", num ) ;  //ถ้าเป็น array กับ pointer ไม่ต้องใส่ &          //รับจำนวนเซตมา

    for( int i = 0 ; i < *num ; i++ ) {                               //เก็บค่าทีละตัวไว้ใน array 
        scanf( "%d", &data[i] ) ; //**แต่บรรทัดนี้ต้องใส่
    }//end for

    for( int i = 0 ; i < *num ; i++ ) {
        printf( "%d ", data[i] ) ;
    }//end for

}//end GetSet
