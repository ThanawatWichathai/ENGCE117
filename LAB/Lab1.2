#include <stdio.h>

void GetSet( int [], int * ) ;

int main() {                   
    
    int data[555], num = 0 ;
    GetSet( data, &num ) ;
    
    return 0 ; //ใส่ return 0 ; เรียบร้อยแล้ว
}//end function 

void GetSet( int data[], int *num ) {                           
    scanf( "%d", num ) ; 

    for( int i = 0 ; i < *num ; i++ ) {                               
        scanf( "%d", &data[i] ) ; 
    }//end for
    
}//end GetSet
