#include <stdio.h>
#include <string.h>

void reverse( char str1[]) ;

int main() {

    char text[50] ;
    char out[50] ;
    reverse( text ) ;
	printf( "%s %s\n", text, out ) ;
    return 0 ;
}//end function

void reverse( char str1[]) {
    gets( str1 ) ;
    int lenght = strlen( str1 ) ;
	int i = 0 ;
	int j = 0 ;
	j = lenght - 1 ;
	char str2 ;

	
	while( i < j ) {
		str2 = str1[i] ;
		str1[i] = str1[j] ;
		str1[j] = str2 ; 
		i++ ;
		j-- ;
	}//end while 
		
}//end function
