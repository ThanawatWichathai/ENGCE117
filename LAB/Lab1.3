#include <stdio.h>

int *GetSet( int * ) ;

int main() {

    int num = 0 ;
    GetSet( &num ) ;

    return 0 ; 
}//end function 

int *GetSet( int *num ) {

    int array[100] ;

    printf( "Enter the number of Elements: " ) ;
    scanf( "%d", num ) ;

    printf( "Enter the Elements: " ) ;
    for( int i = 0 ; i < *num ; i++ ) {
        scanf( "%d", &array[i] ) ;
    }//end for

    printf( "The Number of Element: %d\n", *num ) ;
    for( int i = 0 ; i < *num ; i++ ) {
        printf( "%d ", array[i] ) ;
    }//end for

    return 0 ;
}//end function
